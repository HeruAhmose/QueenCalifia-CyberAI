{
  "uid": "qc-api-kpis",
  "title": "QueenCalifia API KPIs",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests / sec",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(qc_http_requests_total[1m]))"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "p95 latency (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(qc_http_request_duration_seconds_bucket[5m])) by (le))"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP status mix",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(qc_http_requests_total[1m])) by (status)"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      }
    }
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  }
}